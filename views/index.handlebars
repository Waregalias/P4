<h2 class="page-header">Tableau de bord</h2>
<div class="row" ng-app="DashboardModule" ng-controller="DashboardController">
  <div class="col-sm-6">
    <h4>Nouvelle partie</h4>
    <a href="/game/new"><button type="button" class="btn btn-lg btn-success">Lancer la partie</button></a>
  </div>
  <div class="col-sm-6 party-on">
    <h4>Liste des parties en cours</h4>
    <div class="list-group">
      <div class="list-group-item" ng-repeat="rooms in master">
        <h4 class="list-group-item-heading">Lanc√©e par :</h4>
        <a><p class="list-group-item-text" ng-click="join({rooms})">{rooms}</p></a>
      </div>
    </div>
  </div>
</div>
